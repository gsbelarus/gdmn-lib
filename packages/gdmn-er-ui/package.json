{
  "name": "gdmn-er-ui",
  "version": "0.14.14",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@mui/material": "^6.3.1"
  },
  "devDependencies": {
    "gdmn-eslint-config": "workspace:*",
    "gdmn-tsconfig": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gsbelarus/gdmn-lib"
  }
