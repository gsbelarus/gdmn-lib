  "dependencies": {
    "@mui/material": "^7.3.8",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@storybook/addon-links": "^10.2.13",
    "@storybook/react-vite": "^10.2.13",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^25.3.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.27",
    "eslint": "^10.0.2",
    "gdmn-eslint-config": "workspace:*",
    "gdmn-tsconfig": "workspace:*",
    "postcss": "^8.5.6",
    "storybook": "^10.2.13",
    "tailwindcss": "^4.2.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
