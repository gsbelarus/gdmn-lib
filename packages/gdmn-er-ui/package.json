    "@types/node": "^20.19.19",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "gdmn-eslint-config": "workspace:*",
    "gdmn-tsconfig": "workspace:*",
    "postcss": "^8.5.6",
    "storybook": "^9.1.10",
    "tailwindcss": "^3.4.18",
