{
  "name": "gdmn-er-mongo",
  "version": "0.14.209",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "tsx --enable-source-maps --test"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "gdmn-er": "workspace:*",
    "gdmn-eslint-config": "workspace:*",
    "gdmn-tsconfig": "workspace:*",
    "gdmn-utils": "workspace:*",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gsbelarus/gdmn-lib"
  },
  "dependencies": {
    "mongoose": "^9.0.2",
    "zod": "^4.2.1"
  }
}